{
  "name": "partner-reference-application",
  "version": "2.0.0",
  "description": "SAP Partner Reference Application Version 2.0",
  "repository": "github.tools.sap/erp4sme/partner-reference-application.git",
  "author": "SAP",
  "license": "SAP SAMPLE CODE LICENSE",
  "keywords": [
    "BTP",
    "Partner",
    "BusinessTechnologyPlatform"
  ],
  "dependencies": {
    "@sap/cds": "^7.4.2",
    "@sap/cds-common-content": "^1.3.1",
    "@sap/cds-dk": "^7.4.1",
    "@sap/cds-hana": "^2.0.0",
    "@sap/xssec": "^3.6.0",
    "express": "^4.18.2",
    "passport": "^0.7.0"
  },
  "devDependencies": {
    "@cap-js/sqlite": "^1.4.0",
    "@sap/ux-specification": "^1.120.1",
    "axios": "^1.6.2",
    "chai": "^4.3.10",
    "chai-as-promised": "^7.1.1",
    "chai-subset": "^1.6.0",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "mocha": "^10.2.0",
    "pre-commit": "^1.2.2",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5"
  },
  "scripts": {
    "start": "cds-serve",
    "watch-poetryslammanager": "cds watch --open poetryslammanager/webapp/index.html?sap-ui-xx-viewCache=false",
    "undeploy": "cf undeploy poetry-slams --delete-services --delete-service-keys --delete-service-brokers",
    "build": "rimraf resources mta_archives && mbt build --mtar archive",
    "deploy": "cf deploy mta_archives/archive.mtar -f --retries 1",
    "test": "mocha",
    "eslint": "eslint . --ext .js",
    "prettier": "prettier . --write",
    "prettier-check": "prettier . --check"
  },
  "cds": {
    "features": {
      "fetch_csrf": true,
      "assert_integrity": "db"
    },
    "requires": {
      "db": {
        "kind": "sql"
      },
      "uaa": {
        "kind": "xsuaa"
      },
      "[hybrid]": {
        "db": {
          "kind": "hana"
        }
      }
    },
    "hana": {
      "deploy-format": "hdbtable"
    }
  },
  "sapux": [
    "app/poetryslammanager"
  ]
}
